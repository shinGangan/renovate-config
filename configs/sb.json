{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "groupName": "Storybook",
      "groupSlug": "sb",
      "matchPackageNames": ["@nuxtjs/storybook"],
      "matchPackagePrefixes": ["storybook", "@storybook"],
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"]
    }
  ]
}
